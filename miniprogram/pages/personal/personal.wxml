<view class="personal">
  <!-- 头部背景 -->
<view class="personal_header">
  <image src="../../static//personal/bgImg2.jpg"></image>
  <view class="personal_user_container">
  <view>
    <image src="{{userInfo.avatarUrl? userInfo.avatarUrl:'../../static/personal/missing-face.png'}}" class="user" bindtap="toLogin"></image>
    <text>{{userInfo.nickname ? userInfo.nickname:'游客'}}</text>
  </view>
  </view>
  <view class="personal_bg_black_box">
    <view class="personal_bg_black">
      <view class="personal_sivp_box">
        <view class="sivp_left">
          <i class="iconfont icon-wangguan"></i>
          <text>龙大哥</text>
        </view>
        <view class="sivp_right">立即开通</view>
      </view>
      <view class="open_numbership_box">
        <view>我是超级大会员</view>
        <view class="open_numbership_text">开通会员，我就是你的</view>
      </view>
    <image src="../../static/personal/vip-card-bg.png" class="vip_card_bg"></image>
    </view>
  </view>
</view>
<view class="move_container" bindtouchstart="handleStart" bindtouchmove="handleMove" bindtouchend="handleEnd"
style="transform:{{moveDistant}};transition:{{moveTransition}}">
<image src="../../static/personal/arc.png" class="arc"></image>
  <!-- 消息导航 -->
<view class="personal_middle_container">
  <view class="personal_message_content">
    <view class="personal_message_item">
      <i class="iconfont icon-xiaoxi"></i>
      <text>我的消息</text>
    </view>
    <view class="personal_message_item">
      <i class="iconfont icon-biaoqing"></i>
      <text>我的对象</text>
    </view>
    <view class="personal_message_item">
      <i class="iconfont icon-zhuti"></i>
      <text>个人主页</text>
    </view>
    <view class="personal_message_item">
      <i class="iconfont icon-gexingzhuangban"></i>
      <text>个人装扮</text>
    </view>
  </view>
</view>
<!-- 个人中心底部 -->
<view class="personal_bottom_container">
  <view class="personal_bottom_content">
    <view class="personal_bottom_item">
      <text>最近播放</text>
      <text>暂无播放记录</text>
    </view>
    <view class="personal_bottom_item">
      <text>我的音乐</text>
      <i class="iconfont icon-youjiantou"></i>
    </view>
    <view class="personal_bottom_item">
      <text>我的收藏</text>
      <i class="iconfont icon-youjiantou"></i>
    </view>
    <view class="personal_bottom_item">
      <text>我的电台</text>
      <i class="iconfont icon-youjiantou"></i>
    </view>
  </view>
</view>
</view>
</view>